{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react",
				"jest"
    ],
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
				"no-unused-vars": [ "off" ],
				"no-console": ["off"]
    },
		"extends": ["plugin:jest/recommended"]
}
